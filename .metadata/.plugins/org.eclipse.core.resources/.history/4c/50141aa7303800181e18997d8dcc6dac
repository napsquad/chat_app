package Message_Objs;

public class Message_obj extends Generic_Object{

	String contents;
	boolean trade;

	Message_obj()
	{

	}
	
	Message_obj(boolean u, String v, int w, String x, String y, String z)
	{
		   trade = u;
		   contents = v;
		   super.IP = w;
		   super.name = x;
		   super.Sender = y;
		   super.Reciever = z;

	}
}
