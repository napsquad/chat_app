package Message_Objs;

import java.io.Serializable;

public class Message_obj extends Generic_Object implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -3260186153236740519L;
	String contents;
	boolean trade;

	public Message_obj()
	{

	}
	
	public Message_obj(boolean trad, String cont, String sook, String myname)
	{
		   trade = trad;
		   contents = cont;
		   super.IP = sook;
		   super.name = myname;

	}
	
	public String get_contents() {
		return contents;
	}
	public boolean get_bool()
	{
		return trade;
	}
}
